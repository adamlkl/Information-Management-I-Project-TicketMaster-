<?xml version="1.0" ?>

<!DOCTYPE eventlist
[
  <!ELEMENT eventlist (event*)>
  <!ELEMENT event (name, date, act, venue, organiser, ticket)>
  
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT date+ (day, month, year, time)>
  <!ELEMENT act* (#PCDATA)>
  <!ELEMENT venue (name, location)>
  <!ELEMENT eventPartner (#PCDATA)>
  <!ELEMENT ticket (category, price)>
  
  <!ELEMENT day (#PCDATA)>
  <!ELEMENT month (#PCDATA)>
  <!ELEMENT year (#PCDATA)>
  <!ELEMENT time (#PCDATA)>
  
  <!ELEMENT location (#PCDATA)>
  
  <!ELEMENT category (#PCDATA)>
  <!ELEMENT type+ (name, price)>

  <!ELEMENT price (#PCDATA)>
  <!ATTLIST type amountAvailable CDATA #REQUIRED>
  <!ATTLIST event id CDATA #REQUIRED>
]>

